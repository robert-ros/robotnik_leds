<?xml version="1.0"?>

<launch>

	<group ns = "robotnik_leds">
		
		<!-- Load config files to led driver -->

		<rosparam file="$(find robotnik_leds_sdk)/config/led_config.yaml"/>
		<rosparam file="$(find robotnik_leds_sdk)/config/led_state.yaml"/> 

		
		<!-- Launch led device hardware  -->


		 <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
		    <param name="port" value="/dev/ttyACM0"/>
		    <param name="baud" value="2000000"/>
		 </node>


		<!-- Launch ROS led driver API -->

		<node pkg="robotnik_leds_sdk" type="leds_driver.py" name="leds_driver_node" output="screen"/>




	</group>



</launch>